function @@@b_coor_shbuff@@@      (){     
	source dexxgnuparam $@
	echo $(getconf -a | grep PAGESIZE | awk '{printf "%f\n", $2 * 1 / 10}' | awk '{split($0, a, "."); print a[1]}')
}
