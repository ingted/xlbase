function @@@b_dn_wkmem@@@         (){  
	source dexxgnuparam $@
	echo 1024MB
}
function @@@m_coor_gtmprx@@@      (){  
	source dexxgnuparam $@
	dhost=$(awk "(\$6 == \"$cluster\") && (\$2 == \"$specified_cnm\") {print \$4}" $(which dexxhosts))
	chosts=$(awk "(\$6 == \"$cluster\") && (\$4 == \"$dhost\") {print \$2}" $(which dexxhosts))
	hostnid=$(awk "(\$6 == \"$cluster\") && (\$1 == \"$specified_cnm\") {print \$2}" $(which dexxhostroles))
	hostsm=$(awk "(\$6 == \"$cluster\") && (\$1 == \"$specified_cnm\") {print \$5}" $(which dexxhostroles))
	for h in $chosts; do
		hostrolenid=$(awk "(\$6 == \"$cluster\") && (\$1 == \"$h\") {print \$1,\$2,\$3}" $(which dexxhostroles))
		sphrn=(${hostrolenid//\,})
		if [ "${sphrn[2]}" == "gtmprx" ] && [ "${sphrn[1]}" == "$hostnid" ]; then
			echo "${sphrn[0]}"
		fi
	done
}
function @@@m_coor_hostip@@@      (){  
	source dexxgnuparam $@
	awk "(\$6 == \"$cluster\") && (\$2 == \"$specified_cnm\") {print \$1}" dexxhosts
}
function @@@m_coor_pgxcnodename@@@(){  
	source dexxgnuparam $@
	echo $host
}
function @@@m_dn_gtmprx@@@        (){  
	source dexxgnuparam $@
        dhost=$(awk "(\$6 == \"$cluster\") && (\$2 == \"$specified_cnm\") {print \$4}" $(which dexxhosts))
        chosts=$(awk "(\$6 == \"$cluster\") && (\$4 == \"$dhost\") {print \$2}" $(which dexxhosts))
        hostnid=$(awk "(\$6 == \"$cluster\") && (\$1 == \"$specified_cnm\") {print \$2}" $(which dexxhostroles))
        hostsm=$(awk "(\$6 == \"$cluster\") && (\$1 == \"$specified_cnm\") {print \$5}" $(which dexxhostroles))
        for h in $chosts; do
                hostrolenid=$(awk "(\$6 == \"$cluster\") && (\$1 == \"$h\") {print \$1,\$2,\$3}" $(which dexxhostroles))
                sphrn=(${hostrolenid//\,})
                if [ "${sphrn[2]}" == "gtmprx" ] && [ "${sphrn[1]}" == "$hostnid" ]; then
                        echo "${sphrn[0]}"
                fi
        done
}
function @@@m_dn_hostip@@@        (){  
	source dexxgnuparam $@
	awk "(\$6 == \"$cluster\") && (\$2 == \"$specified_cnm\") {print \$1}" dexxhosts
}
function @@@m_dn_pgxcnodename@@@  (){   
	source dexxgnuparam $@
	echo $host
}
